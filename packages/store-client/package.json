{
  "name": "@sker/store-client",
  "version": "0.1.0",
  "description": "SKER Store HTTP Client - 轻量级 Store 服务 HTTP 客户端",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
