{
  "name": "@sker/developer",
  "version": "0.1.0",
  "description": "插件开发平台 - 为@sker/studio提供可扩展的插件生态系统",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "sandbox": "docker-compose -f docker/docker-compose.sandbox.yml up",
    "build:sdk": "cd packages/plugin-sdk && npm run build",
    "publish:sdk": "cd packages/plugin-sdk && npm publish"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@monaco-editor/react": "^4.6.0",
    "@tanstack/react-query": "^5.8.4",
    "antd": "^5.12.8",
    "clsx": "^2.1.1",
    "framer-motion": "^10.16.16",
    "monaco-editor": "^0.44.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "styled-components": "^6.1.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.4"
  },
  "keywords": [
    "plugin-development",
    "developer-tools",
    "SDK",
    "marketplace",
    "code-editor",
    "typescript",
    "react"
  ],
  "author": "SKER Team",
  "license": "MIT"
}